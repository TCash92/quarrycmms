# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
EXPO_PUBLIC_SUPABASE_STORAGE_BUCKET=your-storage-bucket

# Feature flags
EXPO_PUBLIC_ENABLE_VOICE_NOTES=true
EXPO_PUBLIC_ENABLE_OFFLINE_PDF=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# Debug settings
EXPO_PUBLIC_LOG_LEVEL=debug
EXPO_PUBLIC_SHOW_SYNC_DEBUG=true

# Sentry (leave empty to disable in development)
EXPO_PUBLIC_SENTRY_DSN=

# Environment identifier
EXPO_PUBLIC_ENVIRONMENT=development
